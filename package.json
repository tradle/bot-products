{
  "name": "@tradle/bot-products",
  "version": "1.0.0",
  "description": "bot that enables form-driven product purchases",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/bot-products"
  },
  "license": "MIT",
  "author": "mvayngrib",
  "dependencies": {
    "@tradle/bot-require-models": "tradle/bot-require-models#cloud",
    "@tradle/build-resource": "tradle/build-resource",
    "@tradle/custom-models": "https://github.com/tradle/custom-models",
    "@tradle/models": "https://github.com/tradle/models",
    "@tradle/validate-model": "tradle/validate-model",
    "@tradle/validate-resource": "tradle/validate-resource",
    "co": "^4.6.0",
    "cross-env": "^4.0.0",
    "uuid": "^3.0.1",
    "xtend": "^4.0.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@tradle/engine": "^4.9.0",
    "eslint": "^3.15.0",
    "tape": "^4.6.3"
  },
  "scripts": {
    "lint": "eslint --quiet .",
    "start": "cross-env DEBUG=tradle:bots* bots --conf ./conf.json",
    "debug": "cross-env DEBUG=tradle:bots* node --debug --inspect $(which bots) --conf ./conf.json",
    "debug-brk": "cross-env DEBUG=tradle:bots* node --debug-brk --inspect $(which bots) --conf ./conf.json",
    "test": "cross-env NODE_ENV=test DEBUG=tradle:bot* BLUEBIRD_DEBUG=1 node test",
    "test:debug": "cross-env NODE_ENV=test DEBUG=tradle:bot* BLUEBIRD_DEBUG=1 node --debug --inspect test",
    "test:debug-brk": "NODE_ENV=test DEBUG=tradle:bot* BLUEBIRD_DEBUG=1 node --debug-brk --inspect test"
  }
}
